{
  "author": "Szymon Kaliski <hi@szymonkaliski.com>",
  "license": "MIT",
  "name": "neutron",
  "version": "0.0.1",
  "description": "run things quickly in electron",
  "main": "index.js",
  "dependencies": {
    "ansi-parser": "^3.2.7",
    "chokidar": "^1.6.1",
    "johnny-five": "^0.11.1",
    "midi": "^0.9.5",
    "npm": "^5.0.3",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-dom-factories": "^1.0.0",
    "react-dropzone": "^3.13.2",
    "recursive-deps": "^1.0.2",
    "serialport": "^4.0.7",
    "slash": "^1.0.0",
    "through2": "^2.0.3",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "electron-builder": "^18.6.2",
    "electron-prebuilt": "^1.4.13"
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron index.js --not-packaged=true",
    "dev": "electron index.js --not-packaged=true --debug=true",
    "dist:mac": "build --mac",
    "dist:win": "build --win",
    "pack:mac": "build --mac --dir",
    "pack:win": "build --win --dir"
  },
  "bin": {
    "neutron": "./cli.js"
  },
  "build": {
    "appId": "com.szymonkaliski.neutron",
    "win": {
      "target": "portable"
    },
    "fileAssociations": [
      {
        "ext": "js",
        "name": "JavaScript",
        "role": "Viewer"
      }
    ]
  }
}
